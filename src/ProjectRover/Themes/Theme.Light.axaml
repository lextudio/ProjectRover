  <themes:SyntaxColorResource x:Key="SyntaxColor.C#.GetSetAddRemove" Foreground="SaddleBrown" />
  <themes:SyntaxColorResource x:Key="SyntaxColor.C#.GotoKeywords" Foreground="Navy" />
