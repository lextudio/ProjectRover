<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:themes="clr-namespace:ICSharpCode.ILSpy.Themes">
  <SolidColorBrush x:Key="BracketHighlightBackgroundBrush" Color="#280000FF" />
  <Pen x:Key="BracketHighlightBorderPen" Brush="#800000FF" Thickness="1" />
  <SolidColorBrush x:Key="LinkTextForegroundBrush" Color="Blue" />
  <SolidColorBrush x:Key="CurrentLineBackgroundBrush" Color="#1614DCE0" />
  <Pen x:Key="CurrentLineBorderPen" Brush="#3400FF6E" Thickness="1" />
  <SolidColorBrush x:Key="SearchResultBackgroundBrush" Color="LightGreen" />
  <SolidColorBrush x:Key="LineNumbersForegroundBrush" Color="Gray" />

  <SolidColorBrush x:Key="ThemeForegroundBrush" Color="#000000" />
  <SolidColorBrush x:Key="ThemeForegroundLowBrush" Color="#DFDFDF" />
  <SolidColorBrush x:Key="GrayTextBrush" Color="#717171" />
  <SolidColorBrush x:Key="TextBackgroundBrush" Color="#FFFFE1" />
  <SolidColorBrush x:Key="TextForegroundBrush" Color="#FF000000" />
  <SolidColorBrush x:Key="ToolbarBackgroundBrush" Color="#F6F6F6" />
  <SolidColorBrush x:Key="ToolbarBorderBrush" Color="#DFDFDF" />
  <SolidColorBrush x:Key="ToolbarSeparatorBrush" Color="#DFDFDF" />
  <SolidColorBrush x:Key="StatusBarBackgroundBrush" Color="#F6F6F6" />
  <SolidColorBrush x:Key="StatusBarBorderBrush" Color="#DFDFDF" />
  <SolidColorBrush x:Key="StatusBarHoverBrush" Color="#DFDFDF" />
  <SolidColorBrush x:Key="ThemeWarningBackgroundBrush" Color="#FFF9E6" />
  <SolidColorBrush x:Key="ThemeWarningBorderBrush" Color="#F3D378" />
  <SolidColorBrush x:Key="ThemeWarningForegroundBrush" Color="#674500" />
  <SolidColorBrush x:Key="ToolTipBackgroundBrush" Color="#FFFFE1" />
  <SolidColorBrush x:Key="ToolTipBorderBrush" Color="#C2C3C9" />

  <!-- SyntaxColor Begin (migrated from WPF) -->
  <!-- Source: Theme.Light.xaml -->
  <themes:SyntaxColorResource x:Key="SyntaxColor.Asm.Address and Bytes" />
  <themes:SyntaxColorResource x:Key="SyntaxColor.Asm.Comment" Foreground="Green" />
  <themes:SyntaxColorResource x:Key="SyntaxColor.Asm.Directive Operands" Foreground="DarkBlue" FontWeight="Bold" />
  <themes:SyntaxColorResource x:Key="SyntaxColor.Asm.Directives" Foreground="Blue" FontWeight="Bold" />
  <themes:SyntaxColorResource x:Key="SyntaxColor.Asm.Extended Instructions" Foreground="Brown" />
  <themes:SyntaxColorResource x:Key="SyntaxColor.Asm.Instructions" Foreground="Blue" />
  <themes:SyntaxColorResource x:Key="SyntaxColor.Asm.Math Instructions" Foreground="#0080C0" />
  <themes:SyntaxColorResource x:Key="SyntaxColor.Asm.NumberLiteral" Foreground="Orange" />
  <themes:SyntaxColorResource x:Key="SyntaxColor.Asm.Registers" Foreground="#8080FF" Background="#EEEEEE" />
  <themes:SyntaxColorResource x:Key="SyntaxColor.Asm.String" Foreground="Magenta" />
  <themes:SyntaxColorResource x:Key="SyntaxColor.C#.AttributeKeywords" Foreground="Navy" />
