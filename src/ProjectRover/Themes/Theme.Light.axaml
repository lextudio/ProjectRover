  <themes:SyntaxColorResource x:Key="SyntaxColor.C#.Parameter" />
  <themes:SyntaxColorResource x:Key="SyntaxColor.C#.ParameterModifiers" Foreground="DeepPink" FontWeight="Bold" />
  <themes:SyntaxColorResource x:Key="SyntaxColor.C#.Preprocessor" Foreground="Green" />
