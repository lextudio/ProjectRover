  <themes:SyntaxColorResource x:Key="SyntaxColor.C#.TrueFalse" Foreground="DarkCyan" FontWeight="Bold" />
  <themes:SyntaxColorResource x:Key="SyntaxColor.C#.TypeKeywords" Foreground="DarkCyan" FontWeight="Bold" />
  <themes:SyntaxColorResource x:Key="SyntaxColor.C#.TypeParameters" Foreground="#004085" />
  <themes:SyntaxColorResource x:Key="SyntaxColor.C#.UnsafeKeywords" Foreground="Olive" />
  <themes:SyntaxColorResource x:Key="SyntaxColor.C#.ValueTypeKeywords" Foreground="Red" FontWeight="Bold" />
