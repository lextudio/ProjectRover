  <themes:SyntaxColorResource x:Key="SyntaxColor.ILAsm.Label" />
  <themes:SyntaxColorResource x:Key="SyntaxColor.ILAsm.Security" Foreground="Red" />
  <themes:SyntaxColorResource x:Key="SyntaxColor.ILAsm.String" Foreground="Magenta" />
  <themes:SyntaxColorResource x:Key="SyntaxColor.XML.AttributeName" Foreground="Red" />
  <themes:SyntaxColorResource x:Key="SyntaxColor.XML.AttributeValue" Foreground="Blue" />
  <themes:SyntaxColorResource x:Key="SyntaxColor.XML.BrokenEntity" Foreground="Olive" />
  <themes:SyntaxColorResource x:Key="SyntaxColor.XML.CData" Foreground="Blue" />
  <themes:SyntaxColorResource x:Key="SyntaxColor.XML.Comment" Foreground="Green" />
  <themes:SyntaxColorResource x:Key="SyntaxColor.XML.DocType" Foreground="Blue" />
  <themes:SyntaxColorResource x:Key="SyntaxColor.XML.Entity" Foreground="Teal" />
  <themes:SyntaxColorResource x:Key="SyntaxColor.XML.XmlDeclaration" Foreground="Blue" />
  <themes:SyntaxColorResource x:Key="SyntaxColor.XML.XmlTag" Foreground="DarkMagenta" />
