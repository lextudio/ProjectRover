<?xml version="1.0"?>
<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ICSharpCode.ILSpy.Controls"
             x:Class="ICSharpCode.ILSpy.Controls.ResourceStringTable">
  <Grid Margin="5,0,0,0">
    <Grid.RowDefinitions>
      <RowDefinition />
      <RowDefinition />
      <RowDefinition />
    </Grid.RowDefinitions>
    <TextBlock Text="Strings" FontWeight="Bold" FontSize="12" />
    <TextBox x:Name="resourceFilterBox"
             Watermark="Filter..."
             Grid.Row="1"
             TextChanged="OnFilterTextChanged" />
    <ListBox Name="resourceListView" Grid.Row="2" />
  </Grid>
</UserControl>
